[Bastion]
p-tag-bastion ansible_host=103.57.74.21

[all:vars]
ansible_user=ubuntu
ansible_ssh_private_key_file=private-key
ansible_ssh_common_args='-o StrictHostKeyChecking=no -o UserKnownHostsFile=/dev/null -o ProxyCommand="ssh -i private-key -W %h:%p -q ubuntu@103.57.74.21 "'